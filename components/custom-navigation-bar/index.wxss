.navbar {
  position: relative;
}
.nav-wrap {
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  z-index: 1000;
}
.home-icon,
.back-icon {
  flex: 1;
  text-align: center;
  font-size: 32rpx;
}
.back-icon {
  font-size: 42rpx;
}
.nav-title,
.nav-icon {
  position: absolute;
  transform: translate(-50%, -40%);
  left: 50%;
  top: 50%;
  font-size: 0;
}
