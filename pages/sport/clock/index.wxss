.shadow-light {
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
  border-bottom: var(safe-area-bottom);
}
.f2-chart {
  width: 100%;
  height: 500rpx;
}
.border-radius {
  border-radius: 18rpx;
}
.wrap {
  position: relative;
  padding-bottom: 190rpx;
}
.charts-toggle-bar {
  justify-content: space-around;
}
.charts-btn {
  color: #fff;
  width: 130rpx;
  height: 74rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 37rpx;
  font-size: 26rpx;
  transition: font-size 0.5s, background-color 0.5s;
}
.charts-btn.active,
.charts-btn.current,
.charts-btn.on {
  background-color: #fff;
  color: #75acff;
  font-size: 28rpx;
}
.charts-container {
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
  border-bottom: var(safe-area-bottom);
  border-radius: 30rpx;
  padding: 30rpx;
  background-color: #fff;
}
.charts-title {
  text-align: center;
  font-size: 28rpx;
  color: #666;
  line-height: 54rpx;
}
.charts-message {
  text-align: center;
  font-size: 28rpx;
  color: #666;
  line-height: 54rpx;
}
.charts-message .num {
  font-size: 32rpx;
}
.charts-text {
  text-align: center;
  line-height: 54rpx;
  font-size: 24rpx;
  color: #999;
}
.swiper-btn {
  width: 60rpx;
  height: 60rpx;
  background-color: #fff;
  border-radius: 50%;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
  border-bottom: var(safe-area-bottom);
  background-size: 30rpx 30rpx;
  background-position: center;
  background-repeat: no-repeat;
}
.swiper-btn.disable {
  opacity: 0.3;
}
.swiper-btn-prev {
  background-image: url("data:image/svg+xml;utf8,%3Csvg%20width%3D%2224%22%20height%3D%2224%22%20viewBox%3D%220%200%2048%2048%22%20fill%3D%22none%22%20xmlns%3D%22http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg%22%3E%3Crect%20width%3D%2248%22%20height%3D%2248%22%20fill%3D%22white%22%20fill-opacity%3D%220.01%22%2F%3E%3Cpath%20d%3D%22M5.79891%2024L41.7989%2024%22%20stroke%3D%22%23888%22%20stroke-width%3D%224%22%20stroke-linecap%3D%22round%22%20stroke-linejoin%3D%22round%22%2F%3E%3Cpath%20d%3D%22M17.7988%2036L5.79883%2024L17.7988%2012%22%20stroke%3D%22%23888%22%20stroke-width%3D%224%22%20stroke-linecap%3D%22round%22%20stroke-linejoin%3D%22round%22%2F%3E%3C%2Fsvg%3E");
}
.swiper-btn-next {
  background-image: url("data:image/svg+xml;utf8,%3C%3Fxml%20version%3D%221.0%22%20encoding%3D%22UTF-8%22%3F%3E%3Csvg%20width%3D%2224%22%20height%3D%2224%22%20viewBox%3D%220%200%2048%2048%22%20fill%3D%22none%22%20xmlns%3D%22http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg%22%3E%3Crect%20width%3D%2248%22%20height%3D%2248%22%20fill%3D%22white%22%20fill-opacity%3D%220.01%22%2F%3E%3Cpath%20d%3D%22M41.9999%2024H5.99992%22%20stroke%3D%22%23888%22%20stroke-width%3D%224%22%20stroke-linecap%3D%22round%22%20stroke-linejoin%3D%22round%22%2F%3E%3Cpath%20d%3D%22M30%2012L42%2024L30%2036%22%20stroke%3D%22%23888%22%20stroke-width%3D%224%22%20stroke-linecap%3D%22round%22%20stroke-linejoin%3D%22round%22%2F%3E%3C%2Fsvg%3E");
}
.dot {
  width: 6px;
  height: 6px;
  border-radius: 50%;
  background-color: #75acff;
  display: inline-block;
}
.dot.blue {
  background-color: #75acff;
}
.clock-item {
  border-radius: 20rpx;
  padding: 20rpx;
  background-color: #fff;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
  border-bottom: var(safe-area-bottom);
}
.clock-item + .clock-item {
  margin-top: 30rpx;
}
.clock-item .top {
  line-height: calc(60rpx + 1px);
}
.clock-item .top > view {
  display: flex;
  align-items: center;
  height: 60rpx;
}
.clock-item .icon {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  overflow: hidden;
}
.clock-item .text-container {
  width: calc(100% - 60rpx - 20rpx);
  line-height: 1.75;
}
.clock-item .day {
  padding: 0 12rpx;
  border-radius: 20rpx;
  line-height: 36rpx;
  height: 36rpx;
  margin-left: auto;
  display: inline-block;
  border: 1px solid #999;
}
.arrow-right {
  background-size: 30rpx 30rpx;
  background-position: center;
  background-repeat: no-repeat;
  width: 60rpx;
  height: 60rpx;
  background-image: url("data:image/svg+xml;utf8,%3C%3Fxml%20version%3D%221.0%22%20encoding%3D%22UTF-8%22%3F%3E%3Csvg%20width%3D%2224%22%20height%3D%2224%22%20viewBox%3D%220%200%2048%2048%22%20fill%3D%22none%22%20xmlns%3D%22http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg%22%3E%3Crect%20width%3D%2248%22%20height%3D%2248%22%20fill%3D%22white%22%20fill-opacity%3D%220.01%22%2F%3E%3Cpath%20d%3D%22M19%2012L31%2024L19%2036%22%20stroke%3D%22%23999%22%20stroke-width%3D%224%22%20stroke-linecap%3D%22round%22%20stroke-linejoin%3D%22round%22%2F%3E%3C%2Fsvg%3E");
}
.sport-clock {
  width: 170rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #75acff;
  font-size: 28rpx;
  text-align: center;
  color: #fff;
  border-radius: 30rpx;
}
.sport-clock.disable {
  background-color: #d8e1e8;
}
.day-message {
  flex: 1 0 33.3%;
  text-align: center;
}
.chart-title {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666;
  line-height: 40rpx;
  text-align: center;
  margin-bottom: 12rpx;
}
.chart-subtitle view {
  font-size: 60rpx;
  font-family: DINAlternate-Bold, DINAlternate;
  font-weight: bold;
  color: #1e1e1e;
  line-height: 1;
  vertical-align: bottom;
  margin-bottom: 16rpx;
  text-align: center;
}
.chart-subtitle text {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666;
  line-height: 1;
  display: block;
  margin-left: 20rpx;
}
.chart-date {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #bfbfbf;
  line-height: 34rpx;
  text-align: center;
  margin-bottom: 40rpx;
}
