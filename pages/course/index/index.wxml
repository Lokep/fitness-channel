<!--pages/course/index/index.wxml-->
<view class="page">
  <view class="course-container">
    <view class="course-slide" wx:for="{{ courseList }}" wx:key="index">
      <view class="course-title ellipsis"> {{item.title}} </view>
      <view class="course-text ellipsis-3"> {{item.content}} </view>
    </view>
  </view>
</view>
